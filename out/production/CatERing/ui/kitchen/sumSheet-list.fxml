<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.kitchen.SumSheetList">
   <center>
      <ListView fx:id="sheetList" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </center>
   <bottom>
      <FlowPane alignment="CENTER" columnHalignment="CENTER" hgap="10.0" prefHeight="50.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#nuovoButtonPressed" text="Nuovo" />
            <Button mnemonicParsing="false" onAction="#apriButtonPressed" text="Apri" />
            <Button mnemonicParsing="false" onAction="#tabturniButtonPressed" text="Tab. Turni" />
            <Button mnemonicParsing="false" onAction="#fineButtonPressed" text="fine" />
         </children>
      </FlowPane>
   </bottom>
</BorderPane>
